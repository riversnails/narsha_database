    719            *              - 0x04: HSE used as system clock
    720            *              - 0x08: PLL used as system clock
    721            */
    722          uint8_t RCC_GetSYSCLKSource(void)
    723          {
    724            return ((uint8_t)(RCC->CFGR & RCC_CFGR_SWS));
